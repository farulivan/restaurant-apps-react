{
  "env": {
    "browser": true,
    "es2021": true
  },
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "plugins": ["react", "@typescript/eslint"],
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "plugin:react/jsx-runtime",
    "prettier"
  ],
  "ignorePatterns": ["*.cjs"],
  "rules": {
    "no-console": 0,
    "linebreak-style": 0,
    "@typescript-eslint/semi": 0
  }
}
